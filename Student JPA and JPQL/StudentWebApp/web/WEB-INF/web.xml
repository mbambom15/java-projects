<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AddStudentServ</servlet-name>
        <servlet-class>za.ac.tut.controller.AddStudentServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetAllStudentsServ</servlet-name>
        <servlet-class>za.ac.tut.controller.GetAllStudentsServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchStudentServ</servlet-name>
        <servlet-class>za.ac.tut.controller.SearchStudentServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditStudentServ</servlet-name>
        <servlet-class>za.ac.tut.controller.EditStudentServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteStudentServ</servlet-name>
        <servlet-class>za.ac.tut.controller.DeleteStudentServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchName_SurnameServlet</servlet-name>
        <servlet-class>za.ac.tut.controller.SearchName_SurnameServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QualificationSearchServlet</servlet-name>
        <servlet-class>za.ac.tut.controller.QualificationSearchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddStudentServ</servlet-name>
        <url-pattern>/AddStudentServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetAllStudentsServ</servlet-name>
        <url-pattern>/GetAllStudentsServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchStudentServ</servlet-name>
        <url-pattern>/SearchStudentServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditStudentServ</servlet-name>
        <url-pattern>/EditStudentServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteStudentServ</servlet-name>
        <url-pattern>/DeleteStudentServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchName_SurnameServlet</servlet-name>
        <url-pattern>/SearchName_SurnameServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QualificationSearchServlet</servlet-name>
        <url-pattern>/QualificationSearchServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
