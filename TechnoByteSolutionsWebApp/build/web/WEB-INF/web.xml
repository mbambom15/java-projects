<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AddProjectServlet</servlet-name>
        <servlet-class>za.ac.tut.controller.AddProjectServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StartEditServlet</servlet-name>
        <servlet-class>za.ac.tut.controller.StartEditServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EndEditServlet</servlet-name>
        <servlet-class>za.ac.tut.controller.EndEditServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentViewServlet</servlet-name>
        <servlet-class>za.ac.tut.controller.StudentViewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetStatsServlet</servlet-name>
        <servlet-class>za.ac.tut.controller.GetStatsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddProjectServlet</servlet-name>
        <url-pattern>/AddProjectServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StartEditServlet</servlet-name>
        <url-pattern>/StartEditServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EndEditServlet</servlet-name>
        <url-pattern>/EndEditServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentViewServlet</servlet-name>
        <url-pattern>/StudentViewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetStatsServlet</servlet-name>
        <url-pattern>/GetStatsServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
